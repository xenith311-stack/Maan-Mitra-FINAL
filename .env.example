# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google AI API Keys
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Optional: Enhanced AI Services
VITE_GOOGLE_CLOUD_PROJECT_ID=your_gcp_project_id
VITE_GOOGLE_CLOUD_REGION=us-central1
VITE_GOOGLE_CLOUD_VISION_API_KEY=your_vision_api_key_here
VITE_GOOGLE_CLOUD_SPEECH_API_KEY=your_speech_api_key_here
VITE_GOOGLE_CLOUD_TRANSLATE_API_KEY=your_translate_api_key_here

# Feature Flags
VITE_ENABLE_VOICE_FEATURES=true
VITE_ENABLE_VIDEO_FEATURES=true
VITE_ENABLE_GEMINI_AI=true
VITE_ENABLE_FIREBASE=true
VITE_ENABLE_ADVANCED_ANALYTICS=true
VITE_ENABLE_CRISIS_DETECTION=true

# Environment
VITE_APP_ENV=development
VITE_DEBUG=true