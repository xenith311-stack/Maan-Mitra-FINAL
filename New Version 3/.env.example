# Firebase Configuration (MannMitra Mental Health)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google AI API Keys (Required for AI Features)
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Optional: Enhanced AI Services
VITE_GOOGLE_CLOUD_PROJECT_ID=your_cloud_project_id
VITE_GOOGLE_CLOUD_REGION=us-central1
VITE_GOOGLE_CLOUD_VISION_API_KEY=your_vision_api_key_here

# ðŸ”‘ Google Chirp 3 API (Latest HD Speech & Voice Technology)
# Enable Chirp 3 Speech-to-Text and TTS APIs in Google Cloud Console
VITE_GOOGLE_CHIRP3_API_KEY=your_chirp3_api_key_here
VITE_GOOGLE_CLOUD_SPEECH_API_KEY=your_speech_api_key_here

# ðŸ”‘ OPTIONAL: Google Cloud Translation API (Multi-language support)
# Enable Translation API in Google Cloud Console
VITE_GOOGLE_CLOUD_TRANSLATE_API_KEY=your_translate_api_key_here

# Feature Flags
VITE_ENABLE_VOICE_FEATURES=true
VITE_ENABLE_VIDEO_FEATURES=true
VITE_ENABLE_GEMINI_AI=true
VITE_ENABLE_FIREBASE=true
VITE_ENABLE_ADVANCED_ANALYTICS=true
VITE_ENABLE_CRISIS_DETECTION=true
VITE_ENABLE_CHIRP3_HD_VOICES=true

# Environment
VITE_APP_ENV=development
VITE_DEBUG=true